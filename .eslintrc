{
  "plugins": [
    "security"
  ],
  "extends": "airbnb-base",
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-underscore-dangle": [
      0
    ],
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreComments": true
      }
    ],
    "no-console": 0,
    "no-unused-vars": [
      "error"
    ],
    "arrow-body-style": [
      "error", "always"
    ],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1 }],
    "padded-blocks": ["off"],
    "no-restricted-syntax": ["off"]
  }
}
